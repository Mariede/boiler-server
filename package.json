{
  "name": "boiler-server",
  "version": "1.0.0",
  "description": "Boilerplate em express NODE.js",
  "main": "./src/main.js",
  "type": "commonjs",
  "license": "GPL-3.0-only",
  "private": true,
  "engines": {
    "node": ">=12.13.0"
  },
  "outputName": "main.js",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "html-to-text": "^8.0.0",
    "http-proxy": "^1.18.1",
    "log4js": "^6.3.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.13.3",
    "mssql": "^7.1.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.5.0",
    "sharp": "^0.28.3",
    "socket.io": "^4.1.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.31.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-node": "^11.1.0",
    "generate-package-json-webpack-plugin": "^2.1.3",
    "nodemon": "^2.0.12",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-messages": "^2.0.4",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "start": "cls && nodemon ./src/main.js",
    "inspect": "cls && nodemon --inspect ./src/main.js",
    "build": "webpack --config ./.webpack.config.js",
    "forever-win": "set NODE_ENV=development&& forever start -s -c node ./src/main.js"
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "documentation",
      "src/cert/*",
      "src/logs/*",
      "src/queue/*",
      "src/sessions/*",
      "src/uploads/*",
      "src/config.json"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js, json"
  },
  "keywords": [
    "node",
    "express",
    "application",
    "javascript",
    "js",
    "server",
    "back-end"
  ],
  "author": "Michel Guimar√£es Ariede"
}
