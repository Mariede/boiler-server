{
  "name": "node-boiler-server",
  "version": "0.1.0",
  "description": "Boilerplate em express.js",
  "main": "./src/index.js",
  "license": "MIT",
  "private": true,
  "outputName": "main.js",
  "dependencies": {
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "html-to-text": "^5.1.1",
    "http-proxy": "^1.17.0",
    "log4js": "^6.1.2",
    "module-alias": "^2.2.1",
    "mongoose": "^5.9.1",
    "mssql": "^6.1.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.2.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "generate-package-json-webpack-plugin": "^1.0.1",
    "nodemon": "^2.0.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start": "cls && nodemon ./src/index.js",
    "inspect": "cls && nodemon --inspect ./src/index.js",
    "build": "webpack --config .webpack.config.js",
    "proxy": "cls && nodemon _tools/proxy/_proxyStart.js",
    "forever-win": "set NODE_ENV=development&& forever start -s -c node ./src/index.js"
  },
  "nodemonConfig": {
    "ignore": [
      "src/logs/*",
      "src/queue/*",
      "src/sessions/*",
      "src/uploads/*",
      "src/config.json"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js, json"
  },
  "keywords": [
    "node",
    "express",
    "aplicacao",
    "servidor",
    "back-end"
  ],
  "author": "Michel Guimar√£es Ariede"
}
