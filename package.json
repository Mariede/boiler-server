    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.2.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "generate-package-json-webpack-plugin": "^1.0.0",
    "nodemon": "^1.19.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "serve": "cls && set NODE_ENV=development && nodemon API/servidor.js",
