{
  "name": "boiler-server",
  "version": "1.0.0",
  "description": "Boilerplate em express NODE.js",
  "main": "./src/main.js",
  "type": "commonjs",
  "license": "GPL-3.0-only",
  "private": true,
  "engines": {
    "node": ">=12.22.0"
  },
  "outputName": "main.js",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "html-to-text": "^8.2.0",
    "http-proxy": "^1.18.1",
    "log4js": "^6.5.2",
    "module-alias": "^2.2.2",
    "mongoose": "^6.3.6",
    "mssql": "^8.1.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.5.0",
    "sharp": "^0.30.6",
    "socket.io": "^4.5.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.17.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-webpack-plugin": "^3.1.1",
    "generate-package-json-webpack-plugin": "^2.6.0",
    "nodemon": "^2.0.16",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-messages": "^2.0.4",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "start": "cls && nodemon ./src/main.js",
    "inspect": "cls && nodemon --inspect ./src/main.js",
    "build": "webpack --config ./.webpack.config.js",
    "forever-win": "set NODE_ENV=development&& forever start -s -c node ./src/main.js"
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "documentation",
      "src/cert/*",
      "src/logs/*",
      "src/queue/*",
      "src/sessions/*",
      "src/uploads/*",
      "src/config.json"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js, json"
  },
  "keywords": [
    "node",
    "express",
    "application",
    "javascript",
    "js",
    "server",
    "back-end"
  ],
  "author": "Michel Guimar√£es Ariede"
}
