{
  "name": "boiler-server",
  "version": "0.9.5",
  "description": "Boilerplate em express NODE.js",
  "main": "./src/main.js",
  "license": "MIT",
  "private": true,
  "outputName": "main.js",
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "ejs": "^3.1.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "html-to-text": "^5.1.1",
    "http-proxy": "^1.18.0",
    "log4js": "^6.2.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.12",
    "mssql": "^6.2.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.4.0",
    "sharp": "^0.25.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "generate-package-json-webpack-plugin": "^1.0.1",
    "nodemon": "^2.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.7",
    "webpack-messages": "^2.0.4",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start": "cls && nodemon ./src/main.js",
    "inspect": "cls && nodemon --inspect ./src/main.js",
    "build": "webpack --config ./.webpack.config.js",
    "proxy": "cls && nodemon ./_tools/proxy/_proxy-start.js",
    "forever-win": "set NODE_ENV=development&& forever start -s -c node ./src/main.js"
  },
  "nodemonConfig": {
    "ignore": [
      "src/cert/*",
      "src/logs/*",
      "src/queue/*",
      "src/sessions/*",
      "src/uploads/*",
      "src/config.json"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js, json"
  },
  "keywords": [
    "node",
    "express",
    "aplicacao",
    "javascript",
    "js",
    "servidor",
    "back-end"
  ],
  "author": "Michel Guimar√£es Ariede"
}
