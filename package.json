{
  "name": "node-boiler-server",
  "version": "0.0.1",
  "description": "Montando ambiente de teste Node com express.js",
  "main": "./API/servidor.js",
  "outputName": "main.js",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
