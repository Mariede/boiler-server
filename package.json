{
  "name": "boiler-server",
  "version": "1.0.0",
  "description": "Boilerplate em express NODE.js",
  "main": "./src/main.js",
  "type": "commonjs",
  "license": "GPL-3.0-only",
  "private": true,
  "engines": {
    "node": ">=12.22.0"
  },
  "outputName": "main.js",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "html-to-text": "^8.2.1",
    "http-proxy": "^1.18.1",
    "log4js": "^6.9.1",
    "module-alias": "^2.2.3",
    "mongoose": "^6.13.8",
    "mssql": "^8.1.4",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.1",
    "serve-favicon": "^2.5.1",
    "session-file-store": "^1.5.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-webpack-plugin": "^3.2.0",
    "generate-package-json-webpack-plugin": "^2.6.0",
    "nodemon": "^2.0.22",
    "webpack": "^5.101.3",
    "webpack-cli": "^4.10.0",
    "webpack-messages": "^2.0.4",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "start": "cls && nodemon ./src/main.js",
    "inspect": "cls && nodemon --inspect ./src/main.js",
    "build": "webpack --config ./.webpack.config.js",
    "forever-win": "set NODE_ENV=development&& forever start -s -c node ./src/main.js"
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "documentation",
      "src/cert/*",
      "src/logs/*",
      "src/queue/*",
      "src/sessions/*",
      "src/uploads/*",
      "src/config.json"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js, json"
  },
  "keywords": [
    "node",
    "express",
    "application",
    "javascript",
    "js",
    "server",
    "back-end"
  ],
  "author": "Michel Guimar√£es Ariede"
}
